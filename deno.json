{
  "tasks": {
    "test": "deno test -RE tests",
    "update-snapshots": "UPDATE_SNAPSHOTS=1 deno test -REW tests",
    "build": "deno run --allow-all --unstable './tasks/build.ts'",
    "dev": "deno run --allow-all --unstable './tasks/dev.ts'"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.15",
    "@std/fs": "jsr:@std/fs@^1.0.19",
    "std/": "https://deno.land/std@0.174.0/",
    "wire": "https://deno.land/x/wire@v2.0.0-alpha.2/mod.ts"
  },
  "compilerOptions": {
    "lib": ["es2016", "deno.ns"]
  },
  "workspace": ["./tests"]
}
